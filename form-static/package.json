{
  "name": "parcel-rsc-form-widget",
  "version": "0.0.0",
  "private": true,
  "targets": {
    "react-static": {
      "source": "src/Form.tsx",
      "context": "react-server"
    }
  },
  "scripts": {
    "clean": "rm -rf dist public",
    "build": "yarn clean && parcel build --target react-static --no-content-hash --public-url https://p6l-richard.github.io/examples-1/form-static/public --dist-dir public && node scripts/split-output.js",
    "start": "parcel"
  },
  "dependencies": {
    "@parcel/rsc": "^2.14.0",
    "react": "canary",
    "react-dom": "canary"
  },
  "devDependencies": {
    "@parcel/packager-react-static": "2.14.1",
    "@parcel/transformer-react-static": "2.14.1",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "parcel": "^2.14.0",
    "typescript": "^5.4.2"
  }
}
